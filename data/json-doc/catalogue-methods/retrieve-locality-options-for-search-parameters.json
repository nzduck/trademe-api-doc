{
  "endpoint": "RetrieveLocalityOptionsForSearchParameters",
  "title": "Retrieve locality options for search parameters",
  "description": "Retrieves search options for a locality that matches the provided geo-location point.",
  "source": "https://developer.trademe.co.nz/api-reference/catalogue-methods/retrieve-locality-options-for-search-parameters",
  "url": "https://api.trademe.co.nz/v1/SearchOptions/Localities/@{latitude},{longitude}.{file_format}",
  "method": "GET",
  "authentication_required": false,
  "permission_required": "Public",
  "rate_limited": false,
  "supported_formats": [
    "XML",
    "JSON"
  ],
  "parameters": {
    "path": [
      {
        "name": "latitude",
        "type": "String (required)",
        "required": true,
        "description": "The latitude value (in decimal degrees WGS84) of the geographic location."
      },
      {
        "name": "longitude",
        "type": "String (required)",
        "required": true,
        "description": "The longitude value (in decimal degrees WGS84) of the geographic location."
      },
      {
        "name": "file_format",
        "type": "Enumeration (required)",
        "required": true,
        "description": "The format of the response.",
        "enum": [
          {
            "name": "xml",
            "value": "xml",
            "description": "Serialize responses into XML."
          },
          {
            "name": "json",
            "value": "json",
            "description": "Serialize responses into JSON."
          }
        ]
      }
    ],
    "query": []
  },
  "request_body": null,
  "returns": {
    "type": null,
    "description": "Collection of <LocationSearchOption> A list of location keys and options.",
    "fields": [
      {
        "name": "Key",
        "type": "String or null",
        "required": false,
        "description": "The key of the location search parameter."
      },
      {
        "name": "DependentOn",
        "type": "String or null",
        "required": false,
        "description": "If present, the key of the dependent location search parameter."
      },
      {
        "name": "Option",
        "type": "<AttributeOption> or null",
        "required": false,
        "description": "The location search option",
        "nested_fields": [
          {
            "field_name": "Value",
            "field_type": "String or null",
            "description": "The value of the item as a string."
          },
          {
            "field_name": "Display",
            "field_type": "String or null",
            "description": "What should be displayed for this value (often the same)."
          }
        ]
      }
    ]
  },
  "examples": {
    "response_json": [
      {
        "Key": "ABC",
        "DependentOn": "ABC",
        "Option": {
          "Value": "ABC",
          "Display": "ABC"
        }
      },
      {
        "Key": "ABC",
        "DependentOn": "ABC",
        "Option": {
          "Value": "ABC",
          "Display": "ABC"
        }
      }
    ]
  }
}