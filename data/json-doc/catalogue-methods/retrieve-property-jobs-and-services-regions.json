      },
      {
        "name": "Count",
        "type": "Integer",
        "required": false,
        "description": "The number of items that are being sold in this district."
      },
      {
        "name": "SeoDrivenUrl",
        "type": "<SeoDrivenUrl> or null",
        "required": false,
        "description": "Canonical URL to this district"
      },
      {
        "name": "Count",
        "type": "Integer",
        "required": false,
        "description": "The number of items being sold in this region."
      },
      {
        "name": "SeoDrivenUrl",
        "type": "<SeoDrivenUrl> or null",
        "required": false,
        "description": "Canonical URL to this region"
      }
    ]
