        "required": true,
        "description": "The format of the response.",
        "enum": [
          {
            "name": "xml",
            "value": "xml",
            "description": "Serialize responses into XML."
          },
          {
            "name": "json",
            "value": "json",
            "description": "Serialize responses into JSON."
          }
        ]
      }
    ],
    "query": []
  },
  "request_body": null,
  "returns": {
    "type": null,
    "description": "<FavouriteLabelResponse> A response indicating success.",
    "fields": [
      {
        "name": "SearchId",
        "type": "Integer",
        "required": false,
        "description": "The ID of the saved favourite."
      },
      {
        "name": "Label",
        "type": "String or null",
        "required": false,
        "description": "The label of the updated favourite."
      }
    ]
  },
  "examples": {
    "request_json": {
      "Label": "ABC"
    },
    "response_json": {
      "SearchId": 123,
      "Label": "ABC"
