{
  "endpoint": "RetrievesJobPackInvoicesForAMember",
  "title": "Retrieves Job Pack invoices for a member",
  "description": "Retrieves a collection of Job Pack invoices associated with an authenticated member",
  "source": "https://developer.trademe.co.nz/api-reference/jobs-payment-methods/retrieves-job-pack-invoices-for-a-member",
  "url": "https://api.trademe.co.nz/v1/Jobs/Payments/Invoices/JobPacks.{file_format}",
  "method": "GET",
  "authentication_required": true,
  "permission_required": "MyTradeMeRead: Read your membership & listing data.",
  "rate_limited": true,
  "supported_formats": [
    "XML",
    "JSON"
  ],
  "parameters": {
    "path": [
      {
        "name": "file_format",
        "type": "Enumeration (required)",
        "required": true,
        "description": "The format of the response.",
        "enum": [
          {
            "name": "xml",
            "value": "xml",
            "description": "Serialize responses into XML."
          },
          {
            "name": "json",
            "value": "json",
            "description": "Serialize responses into JSON."
          }
        ]
      }
    ],
    "query": [
      {
        "name": "filter",
        "type": "String (optional)",
        "required": false,
        "description": "Optional time based filter. Valid values are All, LastYear, LastSixMonths, LastThreeMonths, LastMonth. Default to LastYear"
      },
      {
        "name": "page",
        "type": "Integer (optional)",
        "required": false,
        "description": "The page number of the set of results to return, starting from 1. Defaults to 1."
      },
      {
        "name": "rows",
        "type": "Integer (optional)",
        "required": false,
        "description": "The number of results per page. The maximum is 50. Defaults to 10."
      }
    ]
  },
  "request_body": null,
  "returns": {
    "type": null,
    "description": "<JobPackInvoiceSummaryCollection> A JobPackInvoiceSummaryCollection containing summary of Job Pack invoices",
    "fields": [
      {
        "name": "TotalCount",
        "type": "Integer",
        "required": false,
        "description": "The total number of results in the collection. Can be larger than the number of returned results."
      },
      {
        "name": "Page",
        "type": "Integer",
        "required": false,
        "description": "The index of the current page of results (starts at 1)."
      },
      {
        "name": "PageSize",
        "type": "Integer",
        "required": false,
        "description": "The number of results in the current page."
      },
      {
        "name": "List",
        "type": "Collection of <JobPackInvoiceSummaryItem> or null",
        "required": false,
        "description": "A list of the results in the current page.",
        "nested_fields": [
          {
            "field_name": "InvoiceId",
            "field_type": "Integer",
            "description": "Invoice Id"
          },
          {
            "field_name": "JobPackTotal",
            "field_type": "Integer",
            "description": "Total number of Job Packs purchased"
          },
          {
            "field_name": "FeatureTotal",
            "field_type": "Integer",
            "description": "Total number of features purchased"
          },
          {
            "field_name": "BrandingTotal",
            "field_type": "Integer",
            "description": "Total number of brandings purchased"
          },
          {
            "field_name": "InvoiceDate",
            "field_type": "DateTime",
            "description": "Date of the invoice"
          },
          {
            "field_name": "TotalExcludingGst",
            "field_type": "Number",
            "description": "Total amount of the invoice excluding Gst"
          },
          {
            "field_name": "TotalIncludingGst",
            "field_type": "Number",
            "description": "Total amount of the invoice including Gst"
          },
          {
            "field_name": "PurchaseOrderNumber",
            "field_type": "String or null",
            "description": "Purchase Order number"
          }
        ]
      }
    ]
  },
  "examples": {
    "response_json": {
      "TotalCount": 123,
      "Page": 123,
      "PageSize": 123,
      "List": [
        {
          "InvoiceId": 123,
          "JobPackTotal": 123,
          "FeatureTotal": 123,
          "BrandingTotal": 123,
          "InvoiceDate": "/Date(1514764800)/",
          "TotalExcludingGst": 123.0,
          "TotalIncludingGst": 123.0,
          "PurchaseOrderNumber": "ABC"
        },
        {
          "InvoiceId": 123,
          "JobPackTotal": 123,
          "FeatureTotal": 123,
          "BrandingTotal": 123,
          "InvoiceDate": "/Date(1514764800)/",
          "TotalExcludingGst": 123.0,
          "TotalIncludingGst": 123.0,
          "PurchaseOrderNumber": "ABC"
        }
      ]
    }
  }
}