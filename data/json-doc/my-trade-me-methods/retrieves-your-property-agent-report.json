{
  "endpoint": "RetrievesYourPropertyAgentReport",
  "title": "Retrieves your property agent report",
  "description": "Retrieves the Property Agent report for the authenticated user.",
  "source": "https://developer.trademe.co.nz/api-reference/my-trade-me-methods/retrieves-your-property-agent-report",
  "url": "https://api.trademe.co.nz/v1/mytrademe/propertyagentreport/{filter}.{file_format}",
  "method": "GET",
  "authentication_required": true,
  "permission_required": "MyTradeMeRead: Read your membership & listing data.",
  "rate_limited": true,
  "supported_formats": [
    "XML",
    "JSON"
  ],
  "parameters": {
    "path": [
      {
        "name": "filter",
        "type": "String (required)",
        "required": true,
        "description": "The range for the report. CurrentAll the current listings.Last45DaysListings in the last 45 days.Last7DaysListings in the last 7 days.AllThe same as Last45Days"
      },
      {
        "name": "file_format",
        "type": "Enumeration (required)",
        "required": true,
        "description": "The format of the response.",
        "enum": [
          {
            "name": "xml",
            "value": "xml",
            "description": "Serialize responses into XML."
          },
          {
            "name": "json",
            "value": "json",
            "description": "Serialize responses into JSON."
          }
        ]
      }
    ],
    "query": []
  },
  "request_body": null,
  "returns": {
    "type": null,
    "description": "<PropertyAgentReport> The Property Agent report data.",
    "fields": [
      {
        "name": "TotalCount",
        "type": "Integer",
        "required": false,
        "description": "The total number of results in the collection. Can be larger than the number of returned results."
      },
      {
        "name": "Page",
        "type": "Integer",
        "required": false,
        "description": "The index of the current page of results (starts at 1)."
      },
      {
        "name": "PageSize",
        "type": "Integer",
        "required": false,
        "description": "The number of results in the current page."
      },
      {
        "name": "List",
        "type": "Collection of <PropertyAgentReportLine> or null",
        "required": false,
        "description": "A list of the results in the current page.",
        "nested_fields": [
          {
            "field_name": "AgentReference",
            "field_type": "String or null",
            "description": "The agent's reference for the listing."
          },
          {
            "field_name": "Title",
            "field_type": "String or null",
            "description": "The title used for the listing."
          },
          {
            "field_name": "Views",
            "field_type": "Integer",
            "description": "The number of views on the listing."
          },
          {
            "field_name": "StartDate",
            "field_type": "DateTime",
            "description": "The UTC start date of the listing."
          },
          {
            "field_name": "EndDate",
            "field_type": "DateTime",
            "description": "The UTC end date of the listing."
          },
          {
            "field_name": "Category",
            "field_type": "String or null",
            "description": "The category of the listing."
          },
          {
            "field_name": "Agent",
            "field_type": "String or null",
            "description": "The agent for the property."
          },
          {
            "field_name": "Address",
            "field_type": "String or null",
            "description": "The address to display."
          },
          {
            "field_name": "Suburb",
            "field_type": "String or null",
            "description": "The suburb name for the property."
          },
          {
            "field_name": "Price",
            "field_type": "String or null",
            "description": "The price description."
          },
          {
            "field_name": "Watchers",
            "field_type": "Integer",
            "description": "The number of views on the property."
          },
          {
            "field_name": "Enquiries",
            "field_type": "Integer",
            "description": "The number of enquiries on the property."
          },
          {
            "field_name": "OpenHomes",
            "field_type": "Integer",
            "description": "The number of open homes."
          },
          {
            "field_name": "IsFeatured",
            "field_type": "Boolean",
            "description": "Indicates whether the property is featured."
          },
          {
            "field_name": "TradeMeReference",
            "field_type": "String or null",
            "description": "The Trade Me property reference."
          }
        ]
      }
    ]
  },
  "examples": {
    "response_json": {
      "TotalCount": 123,
      "Page": 123,
      "PageSize": 123,
      "List": [
        {
          "AgentReference": "ABC",
          "Title": "ABC",
          "Views": 123,
          "StartDate": "/Date(1514764800)/",
          "EndDate": "/Date(1514764800)/",
          "Category": "ABC",
          "Agent": "ABC",
          "Address": "ABC",
          "Suburb": "ABC",
          "Price": "ABC",
          "Watchers": 123,
          "Enquiries": 123,
          "OpenHomes": 123,
          "IsFeatured": false,
          "TradeMeReference": "ABC"
        },
        {
          "AgentReference": "ABC",
