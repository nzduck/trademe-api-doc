{
  "endpoint": "GetAListOfDraftListings",
  "title": "Get a list of draft listings",
  "description": "Get all saved draft listings for member",
  "source": "https://developer.trademe.co.nz/api-reference/selling-methods/get-a-list-of-draft-listings",
  "url": "https://api.trademe.co.nz/v1/Selling/Drafts.{file_format}",
  "method": "GET",
  "authentication_required": true,
  "permission_required": "MyTradeMeWrite: Maintain your watchlist, answer questions and post comments.",
  "rate_limited": true,
  "supported_formats": [
    "XML",
    "JSON"
  ],
  "parameters": {
    "path": [
      {
        "name": "file_format",
        "type": "Enumeration (required)",
        "required": true,
        "description": "The format of the response.",
        "enum": [
          {
            "name": "xml",
            "value": "xml",
            "description": "Serialize responses into XML."
          },
          {
            "name": "json",
            "value": "json",
            "description": "Serialize responses into JSON."
          }
        ]
      }
    ],
    "query": []
  },
  "request_body": null,
  "returns": {
    "type": null,
    "description": "<DraftListingResponse>",
    "fields": [
      {
        "name": "Success",
        "type": "Boolean",
        "required": false,
        "description": "Indicates whether the operation was successful."
      },
      {
        "name": "StatusCode",
        "type": "Enumeration",
        "required": false,
        "description": "The draft listing request status.",
        "enum": [
          {
            "name": "Success",
            "value": "0",
            "description": "The operation was successful."
          },
          {
            "name": "MaxLimitExceeded",
            "value": "1",
            "description": "Max draft limit exceeded failure."
          }
        ]
      },
      {
        "name": "Drafts",
        "type": "Collection of <Draft> or null",
        "required": false,
        "description": "All saved drafts.",
        "nested_fields": [
          {
            "field_name": "DraftId",
            "field_type": "Integer",
            "description": "The ID of the draft listing."
          },
          {
            "field_name": "Name",
            "field_type": "String or null",
            "description": "The name of the draft listing."
          },
          {
            "field_name": "Note",
            "field_type": "String or null",
            "description": "An additional note for the draft listing."
          },
          {
            "field_name": "Listing",
            "field_type": "<ListingRequest> or null",
            "description": "The draft listing data."
          }
        ]
      }
    ]
  },
  "examples": {
    "response_json": {
      "Success": false,
      "StatusCode": 0,
      "Drafts": [
        {
          "DraftId": 123,
          "Name": "ABC",
          "Note": "ABC",
          "Listing": {}
        },
        {
          "DraftId": 123,
          "Name": "ABC",
          "Note": "ABC",
          "Listing": {}
        }
      ]
    }
  }
}